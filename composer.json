{
    "name": "thlib/php-curl-client",
    "description": "A very simple and lightweight cURL helper for PHP",
    "license": "Unlicense",
    "authors": [
        {
            "name": "Timo Huovinen",
            "email": "timoh89@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "autoload": {
        "psr-4": {
            "TH\\CurlClient\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/thlib/php-constants"
        },
        {
            "type": "vcs",
            "url": "https://github.com/thlib/php-url"
        },
        {
            "type": "vcs",
            "url": "https://github.com/thlib/php-curl-multi-basic"
        },
        {
            "type": "vcs",
            "url": "https://github.com/php-fig/http-message"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "thlib/php-constants": "1.0.0",
        "thlib/php-url": "1.0.0",
        "thlib/php-curl-multi-basic": "dev-master",
        "psr/http-message": "1.0.1",
        "slim/psr7": "^1.0"
    }
}
